<h1>login</h1>
<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

    <%# メールアドレス %>
    <%= f.label :email, "メールアドレス" %>
    <%= f.email_field :email %>
    
    <%# パスワード %>
    <%= f.label :password, "パスワード" %>
    <%= f.password_field :password %>

    <%= f.submit "ログイン" %>
    <%= link_to  "新規登録はこちら", signup_path %>
<% end %>